<button type="button" onclick={{action "nameChange"}}>Changing_name</button>

<table>
<thead>
    <th>Name</th>
    <th>Age</th>
    <th>Salary</th>   
</thead>
<tbody>
{{#each employees as |emp index|}}
<tr>
    <td>{{emp.name}}</td> 
         <td>{{emp.age}}</td> 
             <td>{{emp.salary}}</td>             
</tr>
{{/each}}
</tbody>
</table>

<div>
<h2>List of employees</h2>
<ol id="employeelist">
{{#each employees as |emp ind|}}
    <li>
        Name:{{emp.name}}<br>
        Age:{{emp.age}}<br>
        Salary:{{emp.salary}}<br>
    </li>
{{/each}}
</ol>
</div>

