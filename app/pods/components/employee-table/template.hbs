<table id="emp-details" class="form-container">
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Salary</th>
    
    </tr>
  </thead>
  <tbody>
    {{#each employees as |emp|}}
    <tr>
      <td>{{emp.name}}</td>
      <td>{{emp.age}}</td>
      <td>{{emp.salary}}</td>



    </tr>
    {{/each}}
  </tbody>
</table>

<button onclick={{action "onNameClick"}}>change name to faiyaz</button>